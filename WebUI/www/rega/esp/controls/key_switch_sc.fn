function ::CreateContent() {
  object ch = dom.GetObject(chnId);
  string chnAddress = ch.Address();
  WriteLine("<td>");
    WriteLine("<table>");
      WriteLine("<tr id='"#chnId#"scControl' class='hidden'>");
        Call("/esp/controls/key_switch_sc.fn::CreateDoorWindowContact()");
      WriteLine("</tr>");

